      <identifier>remap.pc_printscreen_mprev</identifier>
      <autogen>__KeyToKey__ KeyCode::PC_PRINTSCREEN, ConsumerKeyCode::MUSIC_PREV</autogen>
    </item>
    <item>
      <name>ScrollLock Key to Music Play</name>
      <identifier>remap.pc_scrolllock_mplay</identifier>
      <autogen>__KeyToKey__ KeyCode::PC_SCROLLLOCK, ConsumerKeyCode::MUSIC_PLAY</autogen>
    </item> 
    <item>
      <name>Pause/Break Key to Music Next</name>
      <identifier>remap.pc_pause_mnext</identifier>
      <autogen>__KeyToKey__ KeyCode::PC_PAUSE, ConsumerKeyCode::MUSIC_NEXT</autogen>
    </item>
  </item>
</root>